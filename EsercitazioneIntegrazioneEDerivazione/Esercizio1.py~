import pandas as pd
import numpy as np
import matplotlib.pyplot as plt

#Lettura file 

df_fromfile = pd.read_csv('vel_vs_time.csv')
#df_fromfile

#Creazione grafico velocita in funzione del tempo

ax = df_fromfile['t']

ay = df_fromfile['v']

plt.plot(ax,ay)
plt.plot(ax, ay, 's-', color='limegreen')
plt.set_title('Velocita in funzione del tempo', fontsize=15, color='limegreen')
plt.xlabel('Time')
plt.ylabel('Velocity')
plt.grid(True)

plt.show()
